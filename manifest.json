{
    "manifest_version": 2,
    "name": "Blog Crawler",
    "description": "Blog Crawler",
    "version": "2.1",
    "permissions": [
        "tabs",
        "contextMenus",
        "cookies",
        "notifications",
        "alarms",
        "webNavigation",
        "\u003Call_urls\u003E"
    ],
    "browser_action": {
        "default_icon": "./img/rss.png"
    },
    "background": {
        "scripts": ["background.js"]
    },
    "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
    "options_page": "index.html"
}
