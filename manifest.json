{
    "manifest_version": 2,
    "name": "Creeper",
    "description": "creeper",
    "version": "0.1",
    "permissions": [
        "tabs",
        "contextMenus",
        "cookies",
        "notifications",
        "alarms",
        "webNavigation",
        "\u003Call_urls\u003E"
    ],
    "background": {
        "scripts": [
            "./js/index.js",
            "./js/source.js",
            "./js/jquery.min.js",
            "./js/store.js",
            "./js/visiblity.js"
        ]
    },
    "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
    "options_page": "index.html"
}
